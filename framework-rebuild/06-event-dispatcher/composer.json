{
    "name": "framework-rebuild/event-dispatcher",
    "description": "Chapter 06: Event Dispatcher - Understanding Symfony's Event System",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
