{
    "name": "framework/dependency-injection",
    "description": "Dependency Injection Container implementation",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "psr/container": "^2.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Repository\\": "examples/",
            "App\\Service\\": "examples/",
            "App\\Controller\\": "examples/",
            "App\\Factory\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
